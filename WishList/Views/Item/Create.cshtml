@model WishList.Models.Item

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h3>Add item to wishlist</h3>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
          <input asp-for="Description" />
            <span asp-validation-for="Description"/>
             <span>
                 <button type="submit">Add Item</button>
             </span>
        </form>
    </div>
</div>



